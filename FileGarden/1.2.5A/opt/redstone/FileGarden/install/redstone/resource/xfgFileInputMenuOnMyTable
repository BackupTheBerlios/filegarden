Unmount HDE3=umount /dev/hde3
-----------------seperator2222=none
Floppy=mount /dev/fd0
Unmount Floppy=umount /dev/fd0
CDROM 1=mount /dev/cdrom
Unmount CDROM 1=umount /dev/cdrom
CDROM 2=mount /dev/cdrom1
Unmount CDROM 2=umount /dev/cdrom1

