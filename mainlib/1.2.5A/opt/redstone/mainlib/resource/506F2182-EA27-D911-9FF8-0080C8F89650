echo "\newif\ifpdf" > %Output filename part%.tex ; 
echo "\ifx\pdfoutput\undefined % We're not running pdftex" >> %Output filename part%.tex ; 
echo "\documentclass[dvips,%Font size%pt,%2 colums%,twosided]{%Document class%}" >> %Output filename part%.tex ; 
echo "%%\documentclass[12pt,twosided,openany]{book}" >> %Output filename part%.tex ; 
echo "%%\documentclass[12pt,twosided,titlepage]{book}" >> %Output filename part%.tex ; 
echo "%%\documentclass[12pt,twosided,notitlepage]{book}" >> %Output filename part%.tex ; 
echo "\pdffalse" >> %Output filename part%.tex ; 
echo "\else" >> %Output filename part%.tex ; 
echo "\documentclass[pdftex,%Font size%pt,%2 colums%,twosided,openany]{%Document class%}" >> %Output filename part%.tex ; 
echo "\usepackage{thumbpdf}" >> %Output filename part%.tex ; 
echo "\pdfoutput=1" >> %Output filename part%.tex ; 
echo "\pdftrue" >> %Output filename part%.tex ; 
echo "\pdfcompresslevel=9" >> %Output filename part%.tex ; 
echo "\fi" >> %Output filename part%.tex ; 
echo "" >> %Output filename part%.tex ; 
echo "\title{%Document title header%}" >> %Output filename part%.tex ; 
echo "\author{%Document author name header%}" >> %Output filename part%.tex ; 
echo "\usepackage{geometry}" >> %Output filename part%.tex ; 
echo "\geometry{verbose,%Landscape orientation%,%Document paper%,tmargin=%Top margin in mm%mm,bmargin=%Bottom margin in mm%mm,lmargin=%Left margin in mm%mm,rmargin=%Right margin in mm%mm}" >> %Output filename part%.tex ; 
echo "\usepackage{makeidx}" >> %Output filename part%.tex ; 
echo "\makeindex" >> %Output filename part%.tex ; 
echo "\usepackage{setspace}" >> %Output filename part%.tex ; 
echo "\usepackage[T1]{fontenc}" >> %Output filename part%.tex ; 
echo "\usepackage[latin1]{inputenc}" >> %Output filename part%.tex ; 
echo "\usepackage{fancyhdr}" >> %Output filename part%.tex ; 
echo "\pagestyle{%Page style%}" >> %Output filename part%.tex ; 
echo "\usepackage{lastpage}" >> %Output filename part%.tex ; 
echo "\fancyhead{}" >> %Output filename part%.tex ; 
echo "\fancyfoot{}" >> %Output filename part%.tex ; 
echo "\lhead[]{ }" >> %Output filename part%.tex ; 
echo "\chead[ ]{ }" >> %Output filename part%.tex ; 
echo "\rhead[  ]{}" >> %Output filename part%.tex ; 
echo "\lfoot[]{ }" >> %Output filename part%.tex ; 
echo "\cfoot[ \thepage]{ \thepage}" >> %Output filename part%.tex ; 
echo "\rfoot[ ]{}" >> %Output filename part%.tex ; 
echo "\renewcommand{\headrulewidth}{0mm}" >> %Output filename part%.tex ; 
echo "\renewcommand{\footrulewidth}{0mm}" >> %Output filename part%.tex ; 
echo "\usepackage{%Font%}" >> %Output filename part%.tex ; 
echo "\usepackage{pifont}" >> %Output filename part%.tex ; 
echo "\usepackage{alltt}" >> %Output filename part%.tex ; 
echo "\usepackage{ulem}" >> %Output filename part%.tex ; 
echo "\usepackage{wrapfig}" >> %Output filename part%.tex ; 
echo "\usepackage{multicol}" >> %Output filename part%.tex ; 
echo "\setlength\parskip{0sp}" >> %Output filename part%.tex ; 
echo "\setlength\parindent{0pt}" >> %Output filename part%.tex ; 
echo "\setlength{\columnseprule}{0.00mm}" >> %Output filename part%.tex ; 
echo "\setlength{\columnsep}{20pt}" >> %Output filename part%.tex ; 
echo "\setstretch{%Line space%}" >> %Output filename part%.tex ; 
echo " " >> %Output filename part%.tex ; 
echo "\ifx\pdfoutput\undefined % We're not running pdftex" >> %Output filename part%.tex ; 
echo "\RequirePackage[plainpages=false]{hyperref}" >> %Output filename part%.tex ; 
echo "\usepackage{color}" >> %Output filename part%.tex ; 
echo "\usepackage{epsfig}" >> %Output filename part%.tex ; 
echo "\else" >> %Output filename part%.tex ; 
echo "\RequirePackage[pdftex," >> %Output filename part%.tex ; 
echo "plainpages=false," >> %Output filename part%.tex ; 
echo "pdftitle={%Document title header%}," >> %Output filename part%.tex ; 
echo "pdfauthor={%Document author name header%}," >> %Output filename part%.tex ; 
echo "pdfsubject={%Document title header%}," >> %Output filename part%.tex ; 
echo "pdfkeywords={}," >> %Output filename part%.tex ; 
echo "pdfpagemode={UseOutlines}," >> %Output filename part%.tex ; 
echo "bookmarks,bookmarksopen,hyperfootnotes,hyperindex,backref,nesting," >> %Output filename part%.tex ; 
echo "pdfstartview={FitH}," >> %Output filename part%.tex ; 
echo "colorlinks,linkcolor={blue},citecolor={blue}," >> %Output filename part%.tex ; 
echo "urlcolor={red}," >> %Output filename part%.tex ; 
echo "]{hyperref}" >> %Output filename part%.tex ; 
echo "\def\pdfBorderAttrs{/Border [0 0 0] } % No border arround Links" >> %Output filename part%.tex ; 
echo "\usepackage[pdftex]{epsfig}" >> %Output filename part%.tex ; 
echo "\fi" >> %Output filename part%.tex ; 
echo " " >> %Output filename part%.tex ; 
echo "\begin{document}" >> %Output filename part%.tex ; 
echo "\begin{alltt}" >> %Output filename part%.tex ;
echo " " >> %Output filename part%.tex ; 
echo "{\fontfamily{%Font face if Verbatimised%}\selectfont" >> %Output filename part%.tex ;
cat "%GetCustomCommand%GetApassList2File" >> %Output filename part%.tex ; 
echo "}" >> %Output filename part%.tex ; 
echo "\end{alltt}" >> %Output filename part%.tex ; 
echo "\end{document}" >> %Output filename part%.tex %Output Dvi% %Delete Latex temporary files% ; 
rm -f %GetCustomCommand%GetApassList2File &
