LabelImage=/root/redstone/FileGarden/image/gnome-package-48.png
LabelWidth=185
InputFrom=none
StripPathFromInputFileNames=0
REFRESH=none
ShowAfter=NONE
version=none
SInputFrom=EDITOR : COMMANDER : NONE
