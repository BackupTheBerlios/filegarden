[VALUE]
cmd=%Output type%

[DefaultValue]
Video brightness=0
Video contrast=0
Video hue=0.0
Video color saturation=0.0
VCD track=2
Frame rate ( fps )=25.0
Scale image size=640x480 (square pixel NTSC)

[ActualValue]  
160x120 (minimal small screen)=scale=160:120,
256x192 (small screen)=scale=256:192,  
352x240 (NTSC quarter screen)=scale=352:240,
352x288 (PAL quarter screen)=scale=352:288,
720x480 (standard NTSC)=scale=720:480,  
720x576 (standard PAL)=scale=720:576,  
640x480 (square pixel NTSC)=scale=640:480, 
768x576 (square pixel PAL)=scale=768:576,    
Disable blur filter=0.0
Disable rotate video=%none%
0 for landscape video=rotate=0,
1 for landscape video=rotate=1,
2 for landscape video=rotate=2,
3 for landscape video=rotate=3,
4 for portrait video=rotate=4,
5 for portrait video=rotate=5,
6 for portrait video=rotate=6,
7 for portrait video=rotate=7,
Disable add luma noise=0
Disable add chroma noise=0
Filter all areas=0
Filter edges medium=-15
Filter edges high=-30
Filter flat areas medium=15
Filter flat areas high=30
Raw (uncompress)=echo "#!/bin/bash" > %GetCustomCommand%GetGuids && echo "mencoder mf://%Images location%/*.%Image type% -mf type=%Image type%:fps=%Frame rate ( fps )% -ofps %Frame rate ( fps )% -nosound -o %Mpeg output directory%/%Mpeg output filename%-RAW.avi -ovc raw -ss %Start from N seconds% -endpos %End at N seconds% -vf scale=352:240,framestep=%Renders only every N frame%,hue=%Video hue%:%Video color saturation%,eq=%Video brightness%:%Video contrast%,hqdn3d=%Apply denoise filter to video%,%Mirror video%%Rotate video%noise=%Add luma noise to video%h:%Add chroma noise to video%h,smartblur=%Apply blur filter strength to video%:%Blur filter value%:%Blur filter threshold%,harddup,pp=%Horizontal deblocking%%Vertical deblocking%%Deringing%%Linear blend deinterlacer%%Linear interpolating deinterlace%%Cubic interpolating deinterlacer%%Median deinterlacer%%ffmpeg deinterlacer%  " >> %GetCustomCommand%GetGuids && chmod u+x %GetCustomCommand%GetGuids && xterm -title "Convert multiple images to Raw AVI" -hold -e ./%GetCustomCommand%GetGuids && rm -f %GetCustomCommand%GetGuids &
AVI Movie (Mjpeg AVI)=echo "#!/bin/bash" > %GetCustomCommand%GetGuids && echo "mencoder mf://%Images location%/*.%Image type% -mf type=%Image type%:fps=%Frame rate ( fps )% -ofps %Frame rate ( fps )% -nosound -o %Mpeg output directory%/%Mpeg output filename%-mjpeg.avi -ovc lavc -lavcopts vcodec=mjpeg -ss %Start from N seconds% -endpos %End at N seconds% -vf scale=352:240,framestep=%Renders only every N frame%,hue=%Video hue%:%Video color saturation%,eq=%Video brightness%:%Video contrast%,hqdn3d=%Apply denoise filter to video%,%Mirror video%%Rotate video%noise=%Add luma noise to video%h:%Add chroma noise to video%h,smartblur=%Apply blur filter strength to video%:%Blur filter value%:%Blur filter threshold%,harddup,pp=%Horizontal deblocking%%Vertical deblocking%%Deringing%%Linear blend deinterlacer%%Linear interpolating deinterlace%%Cubic interpolating deinterlacer%%Median deinterlacer%%ffmpeg deinterlacer%   " >> %GetCustomCommand%GetGuids && chmod u+x %GetCustomCommand%GetGuids && xterm -title "Convert multiple images to Mjpeg AVI" -hold -e ./%GetCustomCommand%GetGuids && rm -f %GetCustomCommand%GetGuids &
Mpeg4 (DIVX) --> Standard=echo "#!/bin/bash" > %GetCustomCommand%GetGuids && echo "mencoder mf://%Images location%/*.%Image type% -mf type=%Image type%:fps=%Frame rate ( fps )% -ofps %Frame rate ( fps )% -nosound -o %Mpeg output directory%/%Mpeg output filename%-DIVX.avi -ovc lavc -lavcopts vcodec=mpeg4:mbd=2:keyint=132:v4mv:vqmin=3:vlelim=-4:vcelim=7:lumi_mask=0.07:dark_mask=0.10:naq:vqcomp=0.7:vqblur=0.2:mpeg_quant:vbitrate=2160000 -ss %Start from N seconds% -endpos %End at N seconds% -vf scale=352:240,framestep=%Renders only every N frame%,hue=%Video hue%:%Video color saturation%,eq=%Video brightness%:%Video contrast%,hqdn3d=%Apply denoise filter to video%,%Mirror video%%Rotate video%noise=%Add luma noise to video%h:%Add chroma noise to video%h,smartblur=%Apply blur filter strength to video%:%Blur filter value%:%Blur filter threshold%,harddup,pp=%Horizontal deblocking%%Vertical deblocking%%Deringing%%Linear blend deinterlacer%%Linear interpolating deinterlace%%Cubic interpolating deinterlacer%%Median deinterlacer%%ffmpeg deinterlacer% " >> %GetCustomCommand%GetGuids && chmod u+x %GetCustomCommand%GetGuids && xterm -title "Convert multiple images to Divx" -hold -e ./%GetCustomCommand%GetGuids && rm -f %GetCustomCommand%GetGuids &
msMpeg4v2 --> Microsoft style=echo "#!/bin/bash" > %GetCustomCommand%GetGuids && echo "mencoder mf://%Images location%/*.%Image type% -mf type=%Image type%:fps=%Frame rate ( fps )% -ofps %Frame rate ( fps )% -nosound -o %Mpeg output directory%/%Mpeg output filename%-Mpeg4v2.avi -ovc lavc -lavcopts vcodec=msmpeg4v2:mbd=2:keyint=132:vqblur=1.0:cmp=2:subcmp=2:dia=2:mv0:last_pred=3:vbitrate=2160000 -ss %Start from N seconds% -endpos %End at N seconds% -vf scale=352:240,framestep=%Renders only every N frame%,hue=%Video hue%:%Video color saturation%,eq=%Video brightness%:%Video contrast%,hqdn3d=%Apply denoise filter to video%,%Mirror video%%Rotate video%noise=%Add luma noise to video%h:%Add chroma noise to video%h,smartblur=%Apply blur filter strength to video%:%Blur filter value%:%Blur filter threshold%,harddup,pp=%Horizontal deblocking%%Vertical deblocking%%Deringing%%Linear blend deinterlacer%%Linear interpolating deinterlace%%Cubic interpolating deinterlacer%%Median deinterlacer%%ffmpeg deinterlacer% " >> %GetCustomCommand%GetGuids && chmod u+x %GetCustomCommand%GetGuids && xterm -title "Convert multiple images to MS Divx" -hold -e ./%GetCustomCommand%GetGuids && rm -f %GetCustomCommand%GetGuids &
Windows Media Video 8=echo "#!/bin/bash" > %GetCustomCommand%GetGuids && echo "mencoder mf://%Images location%/*.%Image type% -mf type=%Image type%:fps=%Frame rate ( fps )% -ofps %Frame rate ( fps )% -nosound -o %Mpeg output directory%/%Mpeg output filename%-WMV8.avi -ovc lavc -lavcopts vcodec=wmv2:vbitrate=2160000 -ss %Start from N seconds% -endpos %End at N seconds% -vf scale=352:240,framestep=%Renders only every N frame%,hue=%Video hue%:%Video color saturation%,eq=%Video brightness%:%Video contrast%,hqdn3d=%Apply denoise filter to video%,%Mirror video%%Rotate video%noise=%Add luma noise to video%h:%Add chroma noise to video%h,smartblur=%Apply blur filter strength to video%:%Blur filter value%:%Blur filter threshold%,harddup,pp=%Horizontal deblocking%%Vertical deblocking%%Deringing%%Linear blend deinterlacer%%Linear interpolating deinterlace%%Cubic interpolating deinterlacer%%Median deinterlacer%%ffmpeg deinterlacer% " >> %GetCustomCommand%GetGuids && chmod u+x %GetCustomCommand%GetGuids && xterm -title "Convert multiple images to Window media 8" -hold -e ./%GetCustomCommand%GetGuids && rm -f %GetCustomCommand%GetGuids &
Windows Media Video 7=echo "#!/bin/bash" > %GetCustomCommand%GetGuids && echo "mencoder mf://%Images location%/*.%Image type% -mf type=%Image type%:fps=%Frame rate ( fps )% -ofps %Frame rate ( fps )% -nosound -o %Mpeg output directory%%Mpeg output filename%-WMV7.avi -ovc lavc -lavcopts vcodec=wmv1:vbitrate=2160000 -ss %Start from N seconds% -endpos %End at N seconds% -vf framestep=%Renders only every N frame%,hue=%Video hue%:%Video color saturation%,eq=%Video brightness%:%Video contrast%,hqdn3d=%Apply denoise filter to video%,%Mirror video%%Rotate video%noise=%Add luma noise to video%h:%Add chroma noise to video%h,smartblur=%Apply blur filter strength to video%:%Blur filter value%:%Blur filter threshold%,harddup,pp=%Horizontal deblocking%%Vertical deblocking%%Deringing%%Linear blend deinterlacer%%Linear interpolating deinterlace%%Cubic interpolating deinterlacer%%Median deinterlacer%%ffmpeg deinterlacer% " >> %GetCustomCommand%GetGuids && chmod u+x %GetCustomCommand%GetGuids && xterm -title "Convert multiple images to Window media 7" -hold -e ./%GetCustomCommand%GetGuids && rm -f %GetCustomCommand%GetGuids  " >> %GetCustomCommand%GetGuids && chmod u+x %GetCustomCommand%GetGuids && xterm -title "Convert multiple images to "Windows Media Video 7" -hold -e ./%GetCustomCommand%GetGuids && rm -f %GetCustomCommand%GetGuids &
VCD PAL (Mpeg movie)=mkdir -p %Scratch disk location%/%Mpeg output filename%%GetCustomCommand%GetGuids && cd %Scratch disk location%/%Mpeg output filename%%GetCustomCommand%GetGuids &&  echo "#!/bin/bash" > %GetCustomCommand%GetGuids && echo "mencoder mf://%Images location%/*.%Image type% -mf type=%Image type%:fps=%Frame rate ( fps )% -ofps %Frame rate ( fps )% -nosound -ss %Start from N seconds% -endpos %End at N seconds% -ovc lavc -lavcopts vcodec=mjpeg -vf framestep=%Renders only every N frame%,hue=%Video hue%:%Video color saturation%,eq=%Video brightness%:%Video contrast%,hqdn3d=%Apply denoise filter to video%,%Mirror video%%Rotate video%noise=%Add luma noise to video%h:%Add chroma noise to video%h,smartblur=%Apply blur filter strength to video%:%Blur filter value%:%Blur filter threshold%,harddup,pp=%Horizontal deblocking%%Vertical deblocking%%Deringing%%Linear blend deinterlacer%%Linear interpolating deinterlace%%Cubic interpolating deinterlacer%%Median deinterlacer%%ffmpeg deinterlacer% -o stream.avi && lav2yuv stream.avi | yuvscaler -v 1 -O SIZE_352x240 -n p | mpeg2enc -v 0 -s -f 1 -V 46 -4 2 -2 1  -S 700 -g 6 -G 15 -r 16 -F 3 -a 1 -o stream.mpv && mplex -f 1 -V -b 46 -s 2324 stream.mpv -o %Mpeg output directory%/%Mpeg output filename%.mpg%Remove temporary files%  " >> %GetCustomCommand%GetGuids && chmod u+x %GetCustomCommand%GetGuids && xterm -title "Convert multiple images to VCD PAL Mpeg" -hold -e ./%GetCustomCommand%GetGuids && rm -f %GetCustomCommand%GetGuids &
VCD NTSC (Mpeg movie)=mkdir -p %Scratch disk location%/%Mpeg output filename%%GetCustomCommand%GetGuids && cd %Scratch disk location%/%Mpeg output filename%%GetCustomCommand%GetGuids &&  echo "#!/bin/bash" > %GetCustomCommand%GetGuids && echo "mencoder mf://%Images location%/*.%Image type% -mf type=%Image type%:fps=%Frame rate ( fps )% -ofps %Frame rate ( fps )% -nosound -ss %Start from N seconds% -endpos %End at N seconds% -ovc lavc -lavcopts vcodec=mjpeg -vf framestep=%Renders only every N frame%,hue=%Video hue%:%Video color saturation%,eq=%Video brightness%:%Video contrast%,hqdn3d=%Apply denoise filter to video%,%Mirror video%%Rotate video%noise=%Add luma noise to video%h:%Add chroma noise to video%h,smartblur=%Apply blur filter strength to video%:%Blur filter value%:%Blur filter threshold%,harddup,pp=%Horizontal deblocking%%Vertical deblocking%%Deringing%%Linear blend deinterlacer%%Linear interpolating deinterlace%%Cubic interpolating deinterlacer%%Median deinterlacer%%ffmpeg deinterlacer% -o stream.avi && lav2yuv stream.avi | yuvscaler -v 1 -O SIZE_352x240 -n n | mpeg2enc -v 0 -s -f 1 -V 46 -4 2 -2 1 -S 700 -g 6 -G 15 -r 16 -F 4 -a 1 -o stream.mpv && mplex -f 1 -V -b 46 -s 2324 stream.mpv -o %Mpeg output directory%/%Mpeg output filename%.mpg%Remove temporary files%  " >> %GetCustomCommand%GetGuids && chmod u+x %GetCustomCommand%GetGuids && xterm -title "Convert multiple images to VCD NTSC Mpeg" -hold -e ./%GetCustomCommand%GetGuids && rm -f %GetCustomCommand%GetGuids &
