[CREATE]
Double-spaced input text=checkbox=false=%XFGFormatScanDoubleSpace%
Multiple Horizontal spaces options=combo=false="Multiple horizontal spaces to single space"|"Respect multiple horizontal spaces"
Add line break to end of every lines with=combo=false="none"|"\\"|"\newline"|"\linebreak[4]"


[VALUE]
cmd=%XFGMemoInsertBody%

[DefaultValue]
Multiple Horizontal spaces options=Multiple horizontal spaces to single space

[ActualValue]
\\=%XFGFormatScanInsertRight%=\\
\newline=%XFGFormatScanInsertRight%=\newline
\linebreak[4]=%XFGFormatScanInsertRight%=\linebreak[4]
Multiple horizontal spaces to single space=%none%
Respect multiple horizontal spaces=%XFGFormatScanInsertSpace%=\%XFGFormatScanInsertSpace%
none=%none%
