[CREATE]
Convert image to=combo=false="BMP"|"DCM"|"DCX"|"DIB"|"FAX"|"FITS"|"G3"|"GIF"|"GIF87"|"GRAY"|"ICM"|"ICO"|"ICON"|"JPEG"|"JPG"|"LOGO"|"MIFF"|"MONO"|"PALM"|"PBM"|"PCD"|"PCDS"|"PCL"|"PCT"|"PCX"|"PDB"|"PGM"|"PICT"|"PM"|"PNG"|"PNM"|"PPM"|"PSD"|"RAS"|"SGI"|"SUN"|"SVG"|"TGA"|"TIF"|"TIFF"|"UIL"|"XPM"|"WMF"|"WPG"|"X"|"XBM"|"XCF"|"XWD"

[VALUE]
cmd=/usr/bin/convert %in %out.%Convert image to%

