[ActualValue]
Blast audio volume=+60
Mute audio volume=-200
Source audio volume=+0
AC3=lavc
Mp3=mp3lame -lameopts vbr=3 
160x120 (minimal small screen)=scale=160:120,
256x192 (small screen)=scale=256:192,  
352x240 (NTSC quarter screen)=scale=352:240,
352x288 (PAL quarter screen)=scale=352:288,
720x480 (standard NTSC)=scale=720:480,  
720x576 (standard PAL)=scale=720:576,  
640x480 (square pixel NTSC)=scale=640:480, 
768x576 (square pixel PAL)=scale=768:576,    
Disable blur filter=0.0
Disable rotate video=%none%
0 for landscape video=rotate=0,
1 for landscape video=rotate=1,
2 for landscape video=rotate=2,
3 for landscape video=rotate=3,
4 for portrait video=rotate=4,
5 for portrait video=rotate=5,
6 for portrait video=rotate=6,
7 for portrait video=rotate=7,
Disable add luma noise=0
Disable add chroma noise=0
Filter all areas=0
Filter edges medium=-15
Filter edges high=-30
Filter flat areas medium=15
Filter flat areas high=30
Raw (uncompress)=cd %Mpeg output directory% && echo "#!/bin/bash" > %GetCustomCommand%GetGuids && echo "mencoder vcd://%VCD track% -cdrom-device %GetCustomCommand%GetPlayCDROM -o %Mpeg output filename%-RAW.avi -oac copy -ovc raw -fps %Extraction frame rate ( fps )% -ofps %Extraction frame rate ( fps )% -ss %Start from N seconds% -endpos %End at N seconds% -af volume=%Output AVI audio volume%dB -vf %Scale image size%framestep=%Renders only every N frame%,hue=%Video hue%:%Video color saturation%,eq=%Video brightness%:%Video contrast%,hqdn3d=%Apply denoise filter to video%,%Mirror video%%Rotate video%noise=%Add luma noise to video%h:%Add chroma noise to video%h,smartblur=%Apply blur filter strength to video%:%Blur filter value%:%Blur filter threshold%,harddup,pp=%Horizontal deblocking%%Vertical deblocking%%Deringing%%Linear blend deinterlacer%%Linear interpolating deinterlace%%Cubic interpolating deinterlacer%%Median deinterlacer%%ffmpeg deinterlacer%  " >> %GetCustomCommand%GetGuids && chmod u+x %GetCustomCommand%GetGuids && xterm -title "Extract VCD to RAW AVI" -hold -e ./%GetCustomCommand%GetGuids && rm -f %GetCustomCommand%GetGuids &
AVI Movie (Mjpeg AVI)=echo "#!/bin/bash" > %GetCustomCommand%GetGuids && echo "cd %Mpeg output directory% && mencoder vcd://%VCD track% -cdrom-device %GetCustomCommand%GetPlayCDROM -oac copy -o %Mpeg output filename%-mjpeg.avi -ovc lavc -lavcopts vcodec=mjpeg -fps %Extraction frame rate ( fps )% -ofps %Extraction frame rate ( fps )% -ss %Start from N seconds% -endpos %End at N seconds% -af volume=%Output AVI audio volume%dB -vf %Scale image size%framestep=%Renders only every N frame%,hue=%Video hue%:%Video color saturation%,eq=%Video brightness%:%Video contrast%,hqdn3d=%Apply denoise filter to video%,%Mirror video%%Rotate video%noise=%Add luma noise to video%h:%Add chroma noise to video%h,smartblur=%Apply blur filter strength to video%:%Blur filter value%:%Blur filter threshold%,harddup,pp=%Horizontal deblocking%%Vertical deblocking%%Deringing%%Linear blend deinterlacer%%Linear interpolating deinterlace%%Cubic interpolating deinterlacer%%Median deinterlacer%%ffmpeg deinterlacer%   " >> %GetCustomCommand%GetGuids && chmod u+x %GetCustomCommand%GetGuids && xterm -title "Extract VCD to Mjpeg AVI" -hold -e ./%GetCustomCommand%GetGuids && rm -f %GetCustomCommand%GetGuids &
Mpeg4 (DIVX) --> Standard=echo "#!/bin/bash" > %GetCustomCommand%GetGuids && echo "cd %Mpeg output directory% && mencoder vcd://%VCD track% -cdrom-device %GetCustomCommand%GetPlayCDROM  -o %Mpeg output filename%-DIVX.avi -ovc lavc -lavcopts acodec=ac3:vcodec=mpeg4:autoaspect:mbd=2:keyint=132:v4mv:vqmin=3:vlelim=-4:vcelim=7:lumi_mask=0.07:dark_mask=0.10:naq:vqcomp=0.7:vqblur=0.2:mpeg_quant:vbitrate=2160000 -oac %Audio format for DivX% -fps %Extraction frame rate ( fps )% -ofps %Extraction frame rate ( fps )% -ss %Start from N seconds% -endpos %End at N seconds% -af volume=%Output AVI audio volume%dB -vf %Scale image size%framestep=%Renders only every N frame%,hue=%Video hue%:%Video color saturation%,eq=%Video brightness%:%Video contrast%,hqdn3d=%Apply denoise filter to video%,%Mirror video%%Rotate video%noise=%Add luma noise to video%h:%Add chroma noise to video%h,smartblur=%Apply blur filter strength to video%:%Blur filter value%:%Blur filter threshold%,harddup,pp=%Horizontal deblocking%%Vertical deblocking%%Deringing%%Linear blend deinterlacer%%Linear interpolating deinterlace%%Cubic interpolating deinterlacer%%Median deinterlacer%%ffmpeg deinterlacer% " >> %GetCustomCommand%GetGuids && chmod u+x %GetCustomCommand%GetGuids && xterm -title "Extract VCD to Divx" -hold -e ./%GetCustomCommand%GetGuids && rm -f %GetCustomCommand%GetGuids &
msMpeg4v2 --> Microsoft style=echo "#!/bin/bash" > %GetCustomCommand%GetGuids && echo "cd %Mpeg output directory% && mencoder vcd://%VCD track% -cdrom-device %GetCustomCommand%GetPlayCDROM -o %Mpeg output filename%-Mpeg4v2.avi -ovc lavc -lavcopts acodec=ac3:vcodec=msmpeg4v2:mbd=2:keyint=132:vqblur=1.0:cmp=2:subcmp=2:dia=2:mv0:last_pred=3:vbitrate=2160000 -oac %Audio format for DivX% -fps %Extraction frame rate ( fps )% -ofps %Extraction frame rate ( fps )% -ss %Start from N seconds% -endpos %End at N seconds% -af volume=%Output AVI audio volume%dB -vf format=rgb24,%Scale image size%framestep=%Renders only every N frame%,hue=%Video hue%:%Video color saturation%,eq=%Video brightness%:%Video contrast%,hqdn3d=%Apply denoise filter to video%,%Mirror video%%Rotate video%noise=%Add luma noise to video%h:%Add chroma noise to video%h,smartblur=%Apply blur filter strength to video%:%Blur filter value%:%Blur filter threshold%,harddup,pp=%Horizontal deblocking%%Vertical deblocking%%Deringing%%Linear blend deinterlacer%%Linear interpolating deinterlace%%Cubic interpolating deinterlacer%%Median deinterlacer%%ffmpeg deinterlacer% " >> %GetCustomCommand%GetGuids && chmod u+x %GetCustomCommand%GetGuids && xterm -title "Extract VCD to MS Divx" -hold -e ./%GetCustomCommand%GetGuids && rm -f %GetCustomCommand%GetGuids &
