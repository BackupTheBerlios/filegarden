VCD PAL (Mpeg movie)=mkdir -p %Scratch disk location%/%Mpeg output filename%%GetCustomCommand%GetGuids && echo "" > %Scratch disk location%/%Mpeg output filename%%GetCustomCommand%GetGuids/%Mpeg output filename%.wav && cd %Scratch disk location%/%Mpeg output filename%%GetCustomCommand%GetGuids && echo "#!/bin/bash" > %GetCustomCommand%GetGuids && echo "mencoder vcd://%VCD track% -cdrom-device %GetCustomCommand%GetPlayCDROM -oac copy -fps %Extraction frame rate ( fps )% -ofps %Extraction frame rate ( fps )% -ss %Start from N seconds% -endpos %End at N seconds% -ovc lavc -lavcopts vcodec=mjpeg -af volume=%Output AVI audio volume%dB -vf %Scale image size%framestep=%Renders only every N frame%,hue=%Video hue%:%Video color saturation%,eq=%Video brightness%:%Video contrast%,hqdn3d=%Apply denoise filter to video%,%Mirror video%%Rotate video%noise=%Add luma noise to video%h:%Add chroma noise to video%h,smartblur=%Apply blur filter strength to video%:%Blur filter value%:%Blur filter threshold%,harddup,pp=%Horizontal deblocking%%Vertical deblocking%%Deringing%%Linear blend deinterlacer%%Linear interpolating deinterlace%%Cubic interpolating deinterlacer%%Median deinterlacer%%ffmpeg deinterlacer% -o %Mpeg output filename%.avi && mplayer %Mpeg output filename%.avi -noframedrop -vo yuv4mpeg -ao pcm -waveheader -aofile %Mpeg output filename%.wav && cat stream.yuv | yuvscaler -v 1 -O SIZE_352x288 -n p | mpeg2enc -v 0 -s -f 1 -V 46 -4 2 -2 1 -S 700 -g 6 -G 15 -r 16 -F 3 -a 1 -o %Mpeg output filename%.mpv && mp2enc -b 224 -r 44100 -s -o %With audio% < %Mpeg output filename%.wav && mplex -f 1 -V -b 46 -s 2324 %Mpeg output filename%.mpv %With audio% -o %Mpeg output directory%/%Mpeg output filename%.mpg%Remove temporary files%   " >> %GetCustomCommand%GetGuids && chmod u+x %GetCustomCommand%GetGuids && xterm -title "Extract VCD to VCD PAL Mpeg" -hold -e ./%GetCustomCommand%GetGuids && rm -f %GetCustomCommand%GetGuids &
VCD NTSC (Mpeg movie)=mkdir -p %Scratch disk location%/%Mpeg output filename%%GetCustomCommand%GetGuids && echo "" > %Scratch disk location%/%Mpeg output filename%%GetCustomCommand%GetGuids/%Mpeg output filename%.wav && cd %Scratch disk location%/%Mpeg output filename%%GetCustomCommand%GetGuids && echo "#!/bin/bash" > %GetCustomCommand%GetGuids && echo "mencoder vcd://%VCD track% -cdrom-device %GetCustomCommand%GetPlayCDROM -oac copy -fps %Extraction frame rate ( fps )% -ofps %Extraction frame rate ( fps )% -ss %Start from N seconds% -endpos %End at N seconds% -ovc lavc -lavcopts vcodec=mjpeg -af volume=%Output AVI audio volume%dB -vf %Scale image size%framestep=%Renders only every N frame%,hue=%Video hue%:%Video color saturation%,eq=%Video brightness%:%Video contrast%,hqdn3d=%Apply denoise filter to video%,%Mirror video%%Rotate video%noise=%Add luma noise to video%h:%Add chroma noise to video%h,smartblur=%Apply blur filter strength to video%:%Blur filter value%:%Blur filter threshold%,harddup,pp=%Horizontal deblocking%%Vertical deblocking%%Deringing%%Linear blend deinterlacer%%Linear interpolating deinterlace%%Cubic interpolating deinterlacer%%Median deinterlacer%%ffmpeg deinterlacer% -o %Mpeg output filename%.avi && mplayer %Mpeg output filename%.avi -noframedrop -vo yuv4mpeg -ao pcm -waveheader -aofile %Mpeg output filename%.wav && cat stream.yuv | yuvscaler -v 1 -O SIZE_352x240 -n n | mpeg2enc -v 0 -s -f 1 -V 46 -4 2 -2 1 -S 700 -g 6 -G 15 -r 16 -F 4 -a 1 -o %Mpeg output filename%.mpv && mp2enc -b 224 -r 44100 -s -o %With audio% < %Mpeg output filename%.wav && mplex -f 1 -V -b 46 -s 2324 %Mpeg output filename%.mpv %With audio% -o %Mpeg output directory%/%Mpeg output filename%.mpg%Remove temporary files%   " >> %GetCustomCommand%GetGuids && chmod u+x %GetCustomCommand%GetGuids && xterm -title "Extract VCD to VCD NTSC Mpeg" -hold -e ./%GetCustomCommand%GetGuids && rm -f %GetCustomCommand%GetGuids &
